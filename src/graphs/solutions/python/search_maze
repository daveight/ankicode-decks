```python
import collections
import copy
import functools

class Coordinate:
    def __init__(self, x: int, y: int):
        self.x = x
        self.y = y

WHITE, BLACK = range(2)

def search_maze(maze, s, e):
    # Perform DFS to find a feasible path.
    def search_maze_helper(cur):
        # Checks cur is within maze and is a white pixel.
        if not (0 <= cur.x < len(maze) and 0 <= cur.y < len(maze[cur.x])
                and maze[cur.x][cur.y] == WHITE):
            return False
        path.append(cur)
        maze[cur.x][cur.y] = BLACK
        if cur.x == e.x and cur.y == e.y:
            return True

        if any(map(search_maze_helper,
                    map(Coordinate, (cur.x - 1, cur.x + 1, cur.x, cur.x),
                        (cur.y, cur.y, cur.y - 1, cur.y + 1)))):
            return True
        # Cannot find a path, remove the entry added in path.append(cur).
        del path[-1]
        return False

    path = []
    search_maze_helper(s)
    if len(path) == 0:
        return s.x == e.x and s.y == e.y
    return True
```